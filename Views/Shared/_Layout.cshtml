<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - foodie_mvc</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/foodie_mvc.styles.css" asp-append-version="true" />
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">
    <link rel="canonical" href="https://getbootstrap.com/docs/5.1/examples/dashboard/">
    <link rel="apple-touch-icon" href="getbootstrap.com/docs/5.1/assets/img/favicons/apple-touch-icon.png"
        sizes="180x180">
    <link rel="icon" href="getbootstrap.com/docs/5.1/assets/img/favicons/favicon-32x32.png" sizes="32x32"
        type="image/png">
    <link rel="icon" href="getbootstrap.com/docs/5.1/assets/img/favicons/favicon-16x16.png" sizes="16x16"
        type="image/png">
    <link rel="manifest" href="getbootstrap.com/docs/5.1/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="getbootstrap.com/docs/5.1/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
    <link rel="icon" href="getbootstrap.com/docs/5.1/assets/img/favicons/favicon.ico">
    <meta name="theme-color" content="#7952b3">
</head>

<body>
    <header class="navbar navbar-light sticky-top bg-white flex-md-nowrap p-0 shadow" style="height: 70px;">
        <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" style="height: 70px;" href="#">
            <h3>Food!e360</h3>
        </a>
        <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse"
            data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false"
            aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-nav">
            <div class="nav-item text-nowrap">
                <a class="nav-link px-3" href="#">Sign out</a>
            </div>
        </div>
    </header>
    <div class="container-fluid">
        <div class="row">
            <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse mt-4 shadow">
                <div class="position-sticky pt-3">
                    <ul class="nav flex-column">
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "Home")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Home"
                                asp-action="Index">Home</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Home"
                                asp-action="Index">Home</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "User")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="User"
                                asp-action="Index">User</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="User"
                                asp-action="Index">User</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "StoreType")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="StoreType"
                                asp-action="Index">StoreType</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="StoreType"
                                asp-action="Index">StoreType</a>
                                </li>
                            }

                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "Store")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Store"
                                asp-action="Index">Store</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Store"
                                asp-action="Index">Store</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "ProductType")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="ProductType"
                                asp-action="Index">ProductType</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="ProductType"
                                asp-action="Index">ProductType</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "Product")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Product"
                                asp-action="Index">Product</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Product"
                                asp-action="Index">Product</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "Order")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Order"
                                asp-action="Index">Order</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Order"
                                asp-action="Index">Order</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "OrderedItems")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="OrderedItems"
                                asp-action="Index">OrderedItems</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="OrderedItems"
                                asp-action="Index">OrderedItems</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "Discount")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Discount"
                                asp-action="Index">Discount</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="Discount"
                                asp-action="Index">Discount</a>
                                </li>
                            }
                        }
                        @{
                            if (ViewContext.RouteData.Values["Controller"]!.ToString() == "TransactionHistory")
                            {
                                <li class="nav-item active">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="TransactionHistory"
                                asp-action="Index">TransactionHistory</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark fw-bold" asp-area="" asp-controller="TransactionHistory"
                                asp-action="Index">TransactionHistory</a>
                                </li>
                            }
                        }

                    </ul>

                    <h6
                        class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                        <span>Saved reports</span>
                        <a class="link-secondary" href="#" aria-label="Add a new report">
                            <span data-feather="plus-circle"></span>
                        </a>
                    </h6>
                    <ul class="nav flex-column mb-2">
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <span data-feather="file-text"></span>
                                Create
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <span data-feather="file-text"></span>
                                Details
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <span data-feather="file-text"></span>
                                Edit
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <span data-feather="file-text"></span>
                                Delete
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>

            <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 mt-5">
                <div class="container">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </main>
        </div>
    </div>

    @* <footer class="border-top footer text-muted mt-5">
        <div class="container">
            &copy; 2022 - foodie_mvc - <a asp-area="" asp-controller="Controller" asp-action="Index">Index</a>
        </div>
    </footer> *@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
